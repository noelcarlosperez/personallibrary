  pod 'Reusable'

  target 'PersonalLibraryTests' do
    inherit! :search_paths
    
    pod 'Firebase/Core'
    pod 'Firebase/Database'
    pod 'Firebase/Crash'
    pod 'Firebase/Auth'
    pod 'Firebase/Storage'
    pod 'Firebase/Analytics'
    pod 'ReSwift'
    pod 'Reusable'
    
  end

  target 'PersonalLibraryUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
